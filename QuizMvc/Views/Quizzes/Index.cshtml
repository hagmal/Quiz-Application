
@model IEnumerable<QuizMvc.Models.Quiz>
@{
    ViewData["Title"] = "Quizer";
}


<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css">
    <title>My Shop</title>
</head>
<body>
    <div class="container">
        <h1>Alle Quizer</h1>
        <table class='table table-striped'>
             <tr>
                <th>Id</th>
                <th>Tittel</th>
                <th>Beskrivelse</th>
            </tr>
            @foreach (var quiz in Model)
            {
                <tr>
                    <td>@quiz.QuizId</td>
                    <td>@quiz.Title</td>
                    <td>@quiz.Description</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
